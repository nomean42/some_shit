zb build webos
zb build dune

#find directories in dist
find -maxdepth 1 -type d | grep '[[:digit:]][[:digit:]]\?.[[:digit:]][[:digit:]]\?.[[:digit:]][[:digit:]]\?' | sudo sed 's/\.\///' | while read line
do
cp -r $line /home/ubuntu/builds/$line
done