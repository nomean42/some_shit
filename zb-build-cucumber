find -maxdepth 1 -type d | grep '[[:digit:]].[[:digit:]].[[:digit:]] | sudo sed 's/.///''

#build-script
cd app
npm i
cp app/persik/config/local-config.js.sample app/persik/config/local-config.js
zb build pc
zb build mag250
zb build samsung
